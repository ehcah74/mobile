  power_used_daily_card:
    template: styles_icon_name_state_label
    show_name: true
    show_icon: true
    show_state: true
    color: auto
    name: >-
      [[[ return entity.attributes.friendly_name.replace(' Energy Today', '');
      ]]]
    state_display: |
      [[[ 
        let value = entity.state;
        if (isNaN(value)) return value;
        value = parseFloat(value);
        return value >= 1 
          ? value.toFixed(3) + ' kWh' 
          : (value * 1000).toFixed(0) + ' W';
      ]]]
    styles:
      grid:
        - grid-template-columns: 60px auto
        - align-items: center
        - justify-items: left
      icon:
        - width: 30px
        - height: 30px
        - justify-self: start
        - padding: 0px 10px 0px 10px
        - color: |
            [[[ 
              let power = parseFloat(entity.state);
              if (isNaN(power)) return "var(--disabled-text-color)";
              
              if (power >= 5) {
                return "red";
              } else if (power >= 3.5) {
                return "darkorange";
              } else if (power >= 2) {
                return "gold";
              } else if (power >= 1) {
                return "deepskyblue";
              } else {
                return "limegreen";
              }
            ]]]
      name:
        - font-size: 15px
        - justify-self: start
        - padding-left: 5px
        - color: white
      state:
        - font-size: 14px
        - justify-self: start
        - padding-left: 10px
        - color: white
        - opacity: '.75'
