        if (entity.state == 'playing') {
          let artist = states['media_player'].attributes.media_artist;
          let title = states['media_player'].attributes.media_title;
          if (artist || title) {
            return `${artist}, "${title}"`;
          } else {
            return 'Inactive';
          }
        } else if (entity.state == 'paused') {
          return 'Paused';
        } else if (entity.state == 'standby') {
          return 'Standby';
        } else if (entity.state == 'unavailable') {
          return 'Unavailable';
        } else {
          return 'Off';
        }
      ]]]
    size: 50%
    tap_action:
      action: more-info
    styles:
      grid:
        - grid-template-areas: '"i n l"'
        - grid-template-columns: 65px min-content auto
        - grid-template-rows: min-content min-content min-content
      card:
        - border-radius: 15px
        - margin-top: 10px
        - padding: 15px 0
