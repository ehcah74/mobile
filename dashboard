        html += '<span style="font-size: 15px;  font-weight: 400; opacity: .5; margin: 50px 5px 0px 0px; color: var(--primary-text-color)">'
          + current + '<span style="font-size: 13px; font-weight: 400; opacity: .5;"> C</span></span>';

        return html;
      ]]]
