    layout: icon_name_state2nd
    name: Media Player
    show_label: true
    label: |
      [[[
        if (entity.state == 'playing')
        {
          return 'Playing';
        } else if (entity.state == 'paused')
        {
          return 'Paused';
        } else if (entity.state == 'standby')
        {
          return 'Standby';
        } else if (entity.state == 'unavailable')
        {
          return 'Unavailable';
        } else {
